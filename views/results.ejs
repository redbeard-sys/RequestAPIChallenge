<%- include ('templates/header'); -%>

<div class='container'>
    <div class='jumbotron'>
        <h1> SEARCH RESULTS </h1>
        <button class='btn btn-info' onclick='location.href="/";'>Search Again</button>
    </div>

    <ul id='movies-list'>
        <% data.Search.forEach(function(book){%>
	        <li> 
	            <img class='poster' src=<%=data.cover.large%> />
	            <div>
                    <h5><%=data.title%></h5>
                    <p><%=data.author%></p>
                    <p>publish date: <%data.publish_date%></p>
                    <p>Pages: <%data.number_of_pages%></p>

                    
                    
                    
                </div>
	        </li>	
        <% }); %>
    <ul>
</div>

